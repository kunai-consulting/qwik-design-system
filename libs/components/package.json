{
  "name": "@kunai-consulting/qwik",
  "version": "0.3.0",
  "private": true,
  "description": "A simple example library built by tsdown",
  "type": "module",
  "files": ["dist"],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*/index.js",
      "types": "./dist/*/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.9.1",
    "@types/node": "22.14.0",
    "typescript": "5.4.5",
    "@qwik.dev/core": "https://pkg.pr.new/QwikDev/qwik/@qwik.dev/core@08f10bc",
    "tsdown": "0.13.0",
    "uqr": "^0.1.2",
    "@kunai-consulting/qwik-utils": "workspace:*"
  },
  "dependencies": {
    "@oddbird/css-anchor-positioning": "^0.6"
  },
  "peerDependencies": {
    "@qwik.dev/core": "*"
  }
}
