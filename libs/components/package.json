{
  "name": "@kunai-consulting/qwik",
  "version": "0.3.0",
  "description": "Create a Qwik library",
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "types": "./lib-types/src/index.d.ts"
    }
  },
  "files": ["lib", "lib-types"],
  "private": false,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly --project tsconfig.json --outDir ./lib-types --pretty",
    "dev": "vite --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.9.1",
    "@kunai-consulting/qwik-utils": "workspace:*",
    "@types/node": "22.14.0",
    "typescript": "5.4.5",
    "uqr": "^0.1.2",
    "vite": "^7",
    "vite-tsconfig-paths": "^4.2.1",
    "@qwik.dev/core": "https://pkg.pr.new/QwikDev/qwik/@qwik.dev/core@08f10bc"
  },
  "dependencies": {
    "@oddbird/css-anchor-positioning": "^0.6"
  }
}
